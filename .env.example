# Development
PORT=3000
MONGODB_URI=mongodb://localhost:27017/timetracking

# Google OAuth - Development
DEV_GOOGLE_CLIENT_ID=your_dev_client_id
DEV_GOOGLE_CLIENT_SECRET=your_dev_client_secret
DEV_GOOGLE_REDIRECT_URI=http://localhost:3000/auth/google/callback

# Google OAuth - Production
PROD_GOOGLE_CLIENT_ID=your_prod_client_id
PROD_GOOGLE_CLIENT_SECRET=your_prod_client_secret
PROD_GOOGLE_REDIRECT_URI=https://timetracking.elequin.io/auth/google/callback

# Environment
NODE_ENV=development

# Generate a secure key using: openssl rand -hex 32
ENCRYPTION_KEY=your_secure_encryption_key_here

# MongoDB Configuration
# Generate a secure password using: openssl rand -base64 24
MONGO_ROOT_USERNAME=admin
MONGO_ROOT_PASSWORD=your_secure_password_here 