# Server Configuration
PORT=3000
MONGODB_URI=mongodb://mongodb:27017/timetracking
NODE_ENV=development

# Encryption Key (generate using: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))")
ENCRYPTION_KEY=your_32_character_encryption_key_here

# Google OAuth - Development
DEV_GOOGLE_CLIENT_ID=your_development_client_id
DEV_GOOGLE_CLIENT_SECRET=your_development_client_secret
DEV_GOOGLE_REDIRECT_URI=http://localhost:3000/auth/google/callback

# Google OAuth - Production
PROD_GOOGLE_CLIENT_ID=your_production_client_id
PROD_GOOGLE_CLIENT_SECRET=your_production_client_secret
PROD_GOOGLE_REDIRECT_URI=https://your-production-domain.com/auth/google/callback 